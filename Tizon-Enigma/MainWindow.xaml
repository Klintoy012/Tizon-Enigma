<Window x:Class="Tizon_Enigma.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tizon_Enigma"
        mc:Ignorable="d"
         Title="Enigma Machine" Height="843" Width="1290" Background="#F5F5F5">
    <Grid Margin="2,0,-2,0">
        <StackPanel Margin="-2,2,2,554">
            <Menu Background="#DFD7BF">
                <MenuItem Header="OpenFile">
                    <MenuItem 
        x:Name="MenuOpen" 
        Header="Open"
        Click="MenuOpen_Click"/>
                    <Separator/>
                    <MenuItem 
        x:Name="MenuClose"
        Header="Close"/>
                </MenuItem>

                <!--<MenuItem Header="Settings">
                <MenuItem x:Name="rSettings" Header="Rotor Settings"/>
            </MenuItem>-->
            </Menu>

        </StackPanel>

        <StatusBar Background="#DFD7BF"
VerticalAlignment="Bottom" Margin="-2,0,2,0">
            <StatusBar VerticalAlignment="Bottom" Background="#DFD7BF" >
                <StackPanel Orientation="Horizontal">
                    <TextBlock x:Name="FilePathTextBlock" Text="File Path: " Margin="0,0,20,0"/>
                    <TextBlock x:Name="ringC" Text="Ring Count " Margin="500,0,20,0"/>
                    <TextBlock x:Name="rCount" Text="Character Count per Ring: " Margin="450,0,20,0"/>
                </StackPanel>
            </StatusBar>
        </StatusBar>
